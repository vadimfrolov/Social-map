{{!-- <link rel="stylesheet" href="style.css" --}}
<div class="d-flex flex-row justify-content-around ">
  <div class="icon ">
    <a href="/logout"><img src="/img/logout.png" width="70px"></a>
  </div>
  <div  class="icon ">
    <a href="/newstatus"><img src="/img/newstatus.png" width="70px"></a>
  </div>
  <div  class="icon">
    <a href="/map"><img src="/img/map.png" width="70px"></a>
  </div>
  <div  class="icon">
    <a href="/chat"><img src="/img/chat.png" width="70px"></a>
  </div>
  <div  class="icon">
    <a href="/about"><img src="/img/about.png" width="70px"></a>
  </div>
</div>
<ul class="list">
  {{#each status}}
  <li>
    <a href="/profile/{{this.id}}">{{this.userName}}</a>
  </li>
  <li>
    {{this.date}}
  </li>
  <li>
    {{this.text}}
  </li>
  <li>
    {{this.price}}
  </li>
  <li>
    <a href="/edit/{{this.id}}">edit</a>
  </li>
  <li>
    <a href="/delete/{{this.id}}">delete</a>
  </li>
  {{/each}}
</ul>
